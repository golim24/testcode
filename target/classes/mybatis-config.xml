<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
		
	<typeAliases>
		<typeAlias type="com.spring.web.vo.StBoardVO" alias="StBoard" />
		<typeAlias type="com.spring.web.vo.StResultVO" alias="StResult" />
		<typeAlias type="com.spring.web.vo.CapVO" alias="KoCapList" />
		<typeAlias type="com.spring.web.vo.ChartCountVO" alias="ChartCount" />
		<typeAlias type="com.spring.web.vo.DisBoardVO" alias="DisBoard" />
		<typeAlias type="com.spring.web.vo.UserVO" alias="user" />
