		<typeAlias type="com.spring.web.vo.CommentVO" alias="comment" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/StBoardMapper.xml" />
		<mapper resource="mappers/DisBoardMapper.xml" />
		<mapper resource="mappers/UserMapper.xml" />
	</mappers>
